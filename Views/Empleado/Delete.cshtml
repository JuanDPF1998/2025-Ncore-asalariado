
@{
    ViewData["Title"] = "Delete";
}
@model Empleados

<div class="container">
    <dvi>
        <input type="hidden" asp-for="idEmpleado"/>
        <dl class="row">
            <dl class="col-sm-10">Nombres:</dl>
            <dt class="col-sm-3">@Model.nombres</dt>

            <dl class="col-sm-10">Apellidos:</dl>
            <dt class="col-sm-3">@Model.apellidos</dt>

            <dl class="col-sm-10">Edad:</dl>
            <dt class="col-sm-3">@Model.edad</dt>

            <dl class="col-sm-10">Numero empleado:</dl>
            <dt class="col-sm-3">@Model.dniEmpleado</dt>

            <dl class="col-sm-10">Correo electronico:</dl>
            <dt class="col-sm-2">@Model.email</dt>
        </dl>
        <div>
            <form asp-action="Delete">
                <a class="btn btn-secondary" asp-action="Index">Volver</a>
                <button class="btn btn-success">Eliminar</button>
            </form>
        </div>
    </dvi>
</div>